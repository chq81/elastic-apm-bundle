{
    "name": "goksagun/elastic-apm-bundle",
    "description": "Elastic APM bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "burak.bolat@metglobal.com"
        }
    ],
    "keywords": [
        "elastic apm bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Goksagun\\ElasticApmBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "nipwaayoni/elastic-apm-php-agent": "^7.5",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/security-core": "^5.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
